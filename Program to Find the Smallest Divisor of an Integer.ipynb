    "n = int(input('Enter an integer:'))\n",
    "a = [0]\n",
    "for i in range (2,n+1):\n",
    "    if(n%i == 0):\n",
    "        a.append(i)\n",
    "a.sort()\n",
    "print(\"smallest divisor is :\",a[0])"
